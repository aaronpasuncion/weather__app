{
  "name": "weather_app",
  "version": "1.0.0",
  "description": "weather app",
  "main": "webpack.config.js",
  "homepage": "https://aaronpasuncion.github.io/weather__app/",
  "scripts": {
    "sass": "node-sass ./src/scss/main.scss ./dist/css/style.css -w",
    "compile:sass": "node-sass src/scss/main.scss dist/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" dist/css/style.comp.css -o dist/css/style.prefix.css",
    "compress:css": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development --open",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "weather_app"
  },
  "keywords": [
    "weather",
    "app",
    "aaron",
    "asuncion",
    "aaronpasuncion",
    "aaronasuncion",
    "html",
    "html5",
    "css",
    "css3",
    "js",
    "javascript",
    "es5",
    "es6",
    "sass",
    "scss",
    "wepback",
    "babel",
    "api",
    "portfolio",
    "project"
  ],
  "author": "Aaron P Asuncion",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "concat": "^1.0.3",
    "core-js": "^3.6.5",
    "html-webpack-plugin": "^4.3.0",
    "install": "^0.13.0",
    "npm": "^6.14.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "regenerator-runtime": "^0.13.5",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
